<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:ate="http://www.bigtester.org/schema/ate" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd 
                           http://www.springframework.org/schema/aop 
                           http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
                           http://www.bigtester.org/schema/ate 
                           http://www.bigtester.org/schema/ate/ate.xsd">
	                       
	<!-- this registers beans annotated with @Aspect -->
	<aop:aspectj-autoproxy/>

	<import resource="classpath:META-INF/logger-reporter.xml" />
	<import resource="classpath:META-INF/page_base_bean.xml" />
	
	<ate:regularPage id="publicLoginDashBoard" 
		parent="myBasePageObject" dataFile="dentistSmokeTest/pageObjects/publicLoginDashBoard.data.xml"/>
		
	<ate:pagePropertyCorrectnessAsserter
		id="publicLoginDashBoardPropertyAsserter" resultPage="publicLoginDashBoard" stepERValue="publicLoginDashBoardValidationValue"/>

	<ate:pageElementExistenceAsserter id="publicLoginDashBoardElementExistenceAsserter" resultPage="publicLoginDashBoard" stepERValue="publicLoginDashBoardValidationValue" />

	<ate:stepExpectedResultValue id="publicLoginDashBoardValidationValue" dataValueID="loginDashboardVID"   />

<!-- LogoutLInk myWebElement definition -->
	<ate:myWebElement id="logoutPatientWE" elementFind="logoutPatientEF" elementAction="logoutPatientEA"/>
	<ate:elementFindByXpath id="logoutPatientEF" findByValue="//a[@href='/logoutPatient']" />
	<ate:clickAction id="logoutPatientEA"  />
	
</beans>